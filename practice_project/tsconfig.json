{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES5",
    "sourceMap": true,
    "outDir": "./test_dist",
    "baseUrl": "./test",
    "incremental": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules", "src", "test_dist"]
}
